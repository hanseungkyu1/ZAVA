<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="kr.co.product">
  	<insert id="insert">
  		INSERT INTO zava_product (pid,pname,descript,category,price,stock,mid) 
  		VALUES(#{pid},#{pname},#{descript},#{category},#{price},#{stock},#{mid})
  	</insert>
  
  
  </mapper>